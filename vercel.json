{
    "builds": [
        {
            "src": "medicalSummary/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "medicalSummary/wsgi.py"
        }
    ],
    "buildCommand": "apt-get update && apt-get install -y python3-distutils && pip install -r requirements.txt"
}
