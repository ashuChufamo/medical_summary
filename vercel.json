{
    "builds": [
        {
            "src": "medicalSummary/asgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "medicalSummary/asgi.py"
        }
    ],
    "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
}
